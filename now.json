{
  "functions": {
    "api/**/*.ts": {
      "runtime": "now-deno@0.4.0",
      "memory": 2048
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    }
  ],
  "env": {
    "DENO_VERSION": "latest"
  },
  "build": {
    "env": {
      "DENO_UNSTABLE": "true"
    }
  },
  "regions": ["sin1"]
}