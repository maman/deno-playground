{
  "functions": {
    "api/**/*.ts": {
      "runtime": "now-deno@0.4.0",
      "memory": 1024,
      "includeFiles": "template/**"
    }
  },
  "env": {
    "DENO_VERSION": "latest"
  },
  "build": {
    "env": {
      "DENO_UNSTABLE": "true"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api"
    }
  ],
  "regions": ["sin1"]
}